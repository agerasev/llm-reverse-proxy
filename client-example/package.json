{
  "name": "dashboard-assistant",
  "version": "0.1.0",
  "description": "Dashboard assistant",
  "type": "module",
  "main": "main.mjs",
  "scripts": {
    "clean": "rm -r bundle.js",
    "build": "esbuild src/lib.ts --outfile=bundle.js --format=esm --bundle --sourcemap",
    "build:watch": "esbuild src/lib.ts --outfile=bundle.js --format=esm --bundle --sourcemap --watch",
    "start": "node --enable-source-maps main.mjs"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "esbuild": "0.25.0"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "openai": "^4.86.1"
  }
}